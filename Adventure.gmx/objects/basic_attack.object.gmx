    draw_set_halign(fa_center);
    if (is_real(result)) 
       {
       draw_set_color(c_red);
       }
    else if (result = "BLOCK" || result = "PARRY") draw_set_color(c_blue);
    else if (result = "DODGE") draw_set_color(c_green);
    draw_text(xx, yy, string(result));
    yy -= 1.2;
    delay--;
    draw_set_halign(fa_left);
    dir = point_direction(a.x, a.y, d.x, d.y);
    if (img &lt;= sprite_get_number(spr)) draw_sprite_ext(spr, img, a.x, a.y, .7, .7, dir, noone, 1);
    img++; 
    }

if (delay = 0) 
   {
   if (d.hp &lt;= 0) instance_destroy();
   
   if (a = attacker)
